@article{PLATT201933,
 abstract = {The article centers on close readings of Iurii Tarichâs little studied film Wings of a Serf (1926) and the life and times of the archaeologist Ignatii Iakovlevich Stelletskii (1878â1949), who obsessively sought to discover the legendary library of Ivan the Terrible, thought to lie concealed beneath the Moscow Kremlin. These examples show the peculiar fascination of early Soviet audiences with the tsarist past in its most extreme expressions of violence and cruelty. The alluring, frightening early Soviet Ivan the Terrible demonstrates post-Revolutionary societyâs queasy sense that the excesses of the old regime threatened to reemerge in the new, Soviet era. The conclusion of the article considers how the Stalinist period, both in its unprecedented bloodshed and violence and in its positive myth of Ivan the Terrible as a great leader despite his cruelty, fulfilled this fearful anticipation in ways that could not have been foreseen in the 1920s. This macabre return of a superseded past undergirded the one Stalinist representation of Ivan to revisit the 1920s Gothic image of the tsar, Sergei Eisensteinâs ambivalent portrayal of despotic power and violence in his uncompleted film trilogy Ivan the Terrible (1942â1944).},
 author = {Kevin M.F. Platt},
 doi = {https://doi.org/10.1016/j.ruslit.2019.06.003},
 issn = {0304-3479},
 journal = {Russian Literature},
 keywords = {Gothic, Ivan the Terrible, Ignatii Ia. Stelletskii, Iurii Tarich, Violence, Eisenstein},
 note = {Russian Gothic},
 pages = {33-60},
 title = {Gothic Ivan the Terrible in the 1920s: Iurii Tarichâs Wings of a Serf and the Curious Case of Ignatii Stelletskii},
 url = {https://www.sciencedirect.com/science/article/pii/S0304347919300237},
 volume = {106},
 year = {2019}
}

