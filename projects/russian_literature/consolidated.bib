 number = {2},
 pages = {285-315},
 title = {Writing Poetry Without Words: Pictographic Poems by Rea Nikonova and Sergej Sigej},
 url = {https://www.sciencedirect.com/science/article/pii/S0304347906000299},
 volume = {59},
 year = {2006}
}

