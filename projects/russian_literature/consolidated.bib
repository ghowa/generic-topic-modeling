 keywords = {Skit poetov, Modernity, Civil War, Revolution, Viacheslav Lebedev, Aleksandr Turintsev},
 note = {Locating the Political in Russian and East European Poetry during and after Communism},
 pages = {147-200},
 title = {Civic Poetry in Russian Prague. Making Sense of the Recent Past and Present},
 url = {https://www.sciencedirect.com/science/article/pii/S0304347917300078},
 volume = {87-89},
 year = {2017}
}

