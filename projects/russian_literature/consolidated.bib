@article{TRUBIKHINA2009131,
 abstract = {This essay deals with arguably the most original of contemporary Russian women poets, Elena Shvarts. Shvarts was born in 1948 in St. Petersburg (then Leningrad). Because of the religious connotations of her poetry and a style that shocked Soviet censors, Shvarts's poetry could not be published in Russia officially until 1989. Shvarts's poetry had been widely known before her first official publication in the Soviet Union; it had been published both abroad and in the âSamizdatâ, bringing her early fame and a reputation as one of the most unusual writers of her generation. Since the late 1980s, Shvarts experienced a real surge of creativity; she traveled widely, participated in many literary readings and festivals in Europe and the US, and was the recipient of many literary awards. The focus of this essay is the specific ways in which Shvarts, in her poetry and prose writings, ties together Russian history, ecumenical mysticism, sectarianism, and pagan myth with autobiographical narrative. These characteristics of Shvarts's poetics are considered through the lens of the essay âPoets With History and Poets Without Historyâ by her major poetic predecessor, Marina Tsvetaeva.},
 author = {Julia Trubikhina},
 doi = {https://doi.org/10.1016/j.ruslit.2009.09.006},
 issn = {0304-3479},
 journal = {Russian Literature},
 keywords = {Elena Shvarts, Tsvetaeva},
 number = {1},
 pages = {131-144},
 title = {On the Poetics of Elena Å varc: Poets With and Without History},
 url = {https://www.sciencedirect.com/science/article/pii/S0304347909000489},
 volume = {66},
 year = {2009}
}

@article{TULIAKOVA202035,
 abstract = {The present paper analyses short prose texts written in the 1830sâ1840s under the generic label âlegendâ either in their titles or subtitles. The aim of the research is to ascertain whether such texts followed a similar generic pattern. The article shows that the chosen texts share a network of constituent properties, which indicates their generic cognateness and allows the reader to classify them as literary legends. The Russian literary legend of the 1830sâ1840s may be characterized as a historical narrative relating extraordinary, sometimes criminal events. Though the audience is likely to disbelieve the story, the narrator finds it necessary to communicate it, as it contains an important truth or idea missing from conventional sources. Despite the similarity of the generic pattern, the genre had fuzzy boundaries in the first half of the nineteenth century, which is revealed in numerous title alterations, inconsistency in using cognate labels (âskazanieâ, âpredanieâ, âbylââ), the absence of legend cycles, and a paucity of discussions about the genre. However, it can be concluded that the literary legend of the time was the grain from which the late-nineteenth century genre developed.},
 author = {Natalia Tuliakova},
 doi = {https://doi.org/10.1016/j.ruslit.2020.03.002},
 issn = {0304-3479},
 journal = {Russian Literature},
 keywords = {Literary Legend, Genre Formation, Generic Label, Constituent Properties, 19th Century Russian Literature, Short Prosaic Forms},
 pages = {35-59},
 title = {Literary Legend in Early Nineteenth-Century Russian Literature: Genre Labeling and Genre Formation},
 url = {https://www.sciencedirect.com/science/article/pii/S0304347920300028},
 volume = {111-112},
 year = {2020}
}

