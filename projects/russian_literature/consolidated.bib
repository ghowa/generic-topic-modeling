@article{FIRTICH2009355,
 abstract = {The present essay addresses the âzeroâ symbolism in the Russian avant-garde's discourse, unparalleled in other 20th century avant-gardes, that includes, besides contributions by Malevich and Kruchenykh, ideas and images created by Andrei Belyi and interpretive involvement of Roman Jakobson. This discussion leads to the illumination of the place that Kruchenykh's libretto for Victory over the Sun occupies in the international avant-garde's topos of the symbolic attack on the natural sources of light â the moon and the sun. This âonslaught on the luminariesâ is expressed by what is identified in the essay as the âavant-garde gestureâ of aesthetic defiance, traceable in the international avant-garde from Marinetti to Jean-Luc Godard.},
 author = {Nikolai Firtich},
 doi = {https://doi.org/10.1016/j.ruslit.2009.04.012},
 issn = {0304-3479},
 journal = {Russian Literature},
 keywords = {Kruchenykh, Belyi, Jean-Luc Godard, Jakobson, Malevich, Victory over the Sun},
 number = {1},
 pages = {355-378},
 title = {Rejecting âThe Sun of Cheap Appearancesâ: Journey Beyond âZeroâ with KruÄenych, MaleviÄ, Belyj, Jakobson and Jean-Luc Godard},
 url = {https://www.sciencedirect.com/science/article/pii/S0304347909000131},
 volume = {65},
 year = {2009}
}

@article{FISHZON2018167,
 abstract = {The article takes its basis in D.W. Winnicottâs theories of play and transitional space to argue that Stagnation-era sound recordings and animated films found a devoted audience of both adults and children because, like Winnicottâs good-enough mother or therapist, they created the conditions for playing: they did not merely narrate play, but also embodied, invited, and enacted it. Fishzon looks closely at the performative and analytic functions of Vladimir Vysotskiiâs popular musical adaptation of Alisa v strane chudes (Alice in Wonderland, 1977), which employed and served as a play framework or analytic session. Vysotskiiâs broadly circulated, well-known work enabled Brezhnev-era citizens to dream a new habitus and ideology at a time when irony and playing were becoming increasingly difficult. While opportunities for paradox narrowed in Soviet official discourse and printed fiction after the Thaw, childrenâs records and animated films shaped critical imaginations and made space for subjectivity.},
 author = {Anna Fishzon},
 doi = {https://doi.org/10.1016/j.ruslit.2018.05.007},
 issn = {0304-3479},
 journal = {Russian Literature},
 keywords = {Alice in Wonderland, Lewis Carroll, Vladimir Vysotskii, Sound Recording, Late Socialism, Childrenâs Culture, D.W. Winnicott},
 note = {Russia â Culture of (Non)Conformity: From the Late Soviet Era to the Present},
 pages = {167-193},
 title = {The Place Where We [Want to] Live: East-West and Other Transitional Phenomena in Vladimir Vysotskiiâs Alisa v strane chudes},
 url = {https://www.sciencedirect.com/science/article/pii/S0304347918300218},
 volume = {96-98},
 year = {2018}
}

@article{FIUT2007433,
 abstract = {The paper focuses on Witold Gombrowicz's interpretation of the postcolonial discourse that was taking shape during his years in Argentina. In the Diaries he unmasked the ideological foundations of this discourse. On the one hand, he criticized the very simplistic application by some of its participants of the doctrine of communism, which tends to attribute all evil in the world to Western capitalism, particularly practised by the United States, while on the other hand he voiced objections to the Latin American nativism and nationalism, both engendered, in his view, by an inferiority complex. An emphasis on the language of the body as an interpretational tool in defining the nature of the Latin American identity is a particularly valuable insight on Gombrowicz's part. Ultimately, he opposed the essence-oriented approach to the question of national character and suggested instead an interpersonalist and activist view which, despite traditional forms and stereotypes, emphasizes the need for continual personal and socially oriented self-recreation.},
 author = {Aleksander Fiut},
 doi = {https://doi.org/10.1016/j.ruslit.2007.10.007},
 issn = {0304-3479},
 journal = {Russian Literature},
 keywords = {Gombrowicz, Post-Colonialism},
 number = {4},
 pages = {433-439},
 title = {Gombrowicz the First Post-Colonialist?},
 url = {https://www.sciencedirect.com/science/article/pii/S0304347907001342},
 volume = {62},
 year = {2007}
}

