@article{LORENZ202087,
 abstract = {This article examines Dobroliubovâs early reception of Chernyshevskiiâs aesthetics as expressed in an unpublished essay on literary didacticism and in diary entries from 1857. Critics at the time linked Dobroliubov closely to Chernyshevskii, labeling both of them as adherents of âdidacticâ or âutilitarianâ aesthetics. These private compositions demonstrate, however, that Dobroliubov was reworking Chernyshevskiiâs ideas even before he made his print debut in Sovremennik. I analyze how Dobroliubov enthusiastically defends his elder colleague while at the same time departing from Chernyshevskiiâs iconoclastic, rationalist vision of realityâs superiority over art. Dobroliubov melds the âorganicâ perspective of Belinskiiâs aesthetics with the views expressed in Chernyshevskiiâs dissertation to produce a moderated utilitarian theory that retains reverence for the special powers of art. Dobroliubovâs vision of the relationship between art and reality is also inflected by his youthful sense that âlifeâ was just beginning to reveal exciting professional and romantic potential. The article offers a nuanced understanding of the utilitarianism in Dobroliubovâs aesthetics, distinguishes him clearly from his sometimes more prominent elder colleague, and contributes to a fuller understanding of aesthetic utilitarianism and mid-nineteenth-century Russian culture in general.},
 author = {Sarah Ruth Lorenz},
 doi = {https://doi.org/10.1016/j.ruslit.2020.04.006},
 issn = {0304-3479},
 journal = {Russian Literature},
