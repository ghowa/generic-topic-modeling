 title = {Jurij Lotman and the Semiotics of Culture},
 url = {https://www.sciencedirect.com/science/article/pii/0304347977900199},
 volume = {5},
 year = {1977}
}

@article{SHVABRIN200999,
 abstract = {In âNotes on Prosodyâ (1964), Vladimir Nabokov's most detailed delineation of his views on versification, special attention is given to âpausative verseâ. The present discussion of the semantic aura this meter had for Nabokov delves into the origins of his notion of the phenomenon, showing Nabokov's indebtedness to Sergei Bobrov's experimental writings on verse theory, especially to Bobrov's substantiation of the term âpauznikâ in his numerous pre-Revolutionary publications on poetics. The outwardly paradoxical nature of Nabokov's attitude toward âpausative verseâ was characterized, on the one hand, by his appreciation of this medium, and his avoidance of it in his own poetic praxis, on the other. It also can be called a focal point where various aspects of Nabokov's appreciation for Aleksandr Blok, the meter's most influential practitioner, come together. Making use of published as well as unpublished documents that shed light on Nabokov's notion of Blok's most lasting achievement, the present article shows that Nabokov's relationship with Blok cannot be satisfactorily interpreted along the lines of Harold Bloom's âanxiety of influenceâ, while an examination of Nabokov's translated oeuvre demonstrates that our present idea of his apparent aversion to accentual verse is in need of reconsideration.},
 author = {Stanislav Shvabrin},
 doi = {https://doi.org/10.1016/j.ruslit.2009.09.005},
 issn = {0304-3479},
 journal = {Russian Literature},
 keywords = {Nabokov, Pausative Verse, Bobrov, Blok},
 number = {1},
 pages = {99-130},
 title = {âThe Dreamy Stutter of Blok's Rhythmsâ: On the Name and Nature of Nabokov's Notion of âPausative Verseâ},
 url = {https://www.sciencedirect.com/science/article/pii/S0304347909000477},
 volume = {66},
 year = {2009}
}

@article{SHVABRIN2013363,
 abstract = {NabokovÊ¼s admission in a 1971 interview that at some point in 1918, before leaving Russia, he translated certain lieder by Heinrich Heine (1797â1856) has been cited in a number of contexts. Almost invariably, however, this is done with the all-too-predictable aim of casting into doubt NabokovÊ¼s self-professed lack of knowledge of German on the one hand, and to serve as a springboard for highly speculative suppositions concerning the writerÊ¼s alleged indebtedness to this or that putative German source, on the other. Surprisingly, prior to the commencement of the research effort that lent impetus to the present study, no attempt has been made to delve into the facts and consequences of NabokovÊ¼s creative engagement with the giant of the German cultural tradition whose distinct kind of lyricism and poetic diction exerted a singularly powerful, indeed formative, influence on Russian letters. Below I introduce and interpret concrete textual data pertaining to NabokovÊ¼s dialogue with the German-Jewish poet that first took place in the medium of translation, continued in NabokovÊ¼s original work written in his imitation, and eventually left an indelible mark on some of the writerÊ¼s most consequential creations. NabokovÊ¼s contribution to Russian Heineana, his life-long appreciation of Heine, shed new and unexpected light on the writerÊ¼s relationship with his artistic paragons, while representing a fascinating manifestation of Nabokovian intertextuality.},
