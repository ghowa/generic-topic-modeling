@article{ZUBKOV202033,
 abstract = {Aleksandr Ostrovskii, the best known and very prolific Russian playwright of the nineteenth century, interacted constantly with the officials responsible for dramatic censorship, considered by most of Ostrovskiiâs contemporaries the most brutal branch of the censorship apparatus. The censor and, independently, Nicholas I personally, did not allow Ostrovskii to stage his first major play. A similar fate awaited some of his later plays. By the mid-1850âs, Ostrovskii had accumulated significant symbolic capital and officials from the Censorship Department started avoiding conflicts with him. Instead, they tried to draw him to their side by interpreting his works in a light that would render them suitable to the requirements for dramatic works. Ostrovskii, from the beginning of his career, tried not so much to avoid conflicts with the censorship and cuts to his plays as to provide the censor with a way to interpret his works that would not require them to be banned. The present article analyzes both the history of such attempts by Ostrovskii and his censors to find common ground, and the reasons why these attempts turned out to be not entirely effective. Using material from the censorship archives, I attempt to demonstrate that such attempts changed the playwrightâs status.},
 author = {Kirill Zubkov},
 doi = {https://doi.org/10.1016/j.ruslit.2020.04.004},
 issn = {0304-3479},
 journal = {Russian Literature},
 keywords = {Russian Literature, A.N. Ostrovskii, Censorship, Theatre, 19th Century Theatre, I.A. Nordstrem},
 note = {Censorship in Nineteenth-Century Russia},
 pages = {33-59},
 title = {Aleksander Ostrovskii and the Censor: Between Oppression and Symbiosis},
 url = {https://www.sciencedirect.com/science/article/pii/S0304347920300211},
 volume = {113},
 year = {2020}
}

@article{ZUBKOV202171,
 abstract = {The paper concerns representations of the peasantry in Russian plays of 1855â1876. It is focused on a group of plays that was submitted for the âUvarov Awardâ, the first literary prize in Russia. This unique source allows us to compare plays by outstanding playwrights and professional stage writers of the time, who provided theaters with ample production for everyday use, with writings by amateurs. The authors focus on a common pattern of representation, in which peasants transgress the borders of their estate (soslovie); most plays focus on marriage or a love story that forces a peasant woman to change her social status. The recurring employment of these tropes was most likely used to reflect the rapid growth of social mobility and the weakening of stable borders of estates in the age of reforms of Alexander II. The most outspoken version of this transgression can be discovered in award-winning The Bitter Fate by A.F. Pisemskii. The members of the committee that was responsible for distributing the awards believed that the play allowed viewers of different social strata to join, thus transgressing the borders of estates not only on the stage, but also in the auditorium.},
