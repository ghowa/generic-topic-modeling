@article{SHUKMAN197741,
 abstract = {This article is concerned with four of Lotman's most recent articles on the semiotics of culture and the background to them. While Lotman's work on literary theory has become widely known and translated, and his work on Russian literature is familiar to many students of Russian culture, his reputation as a leading figure in the world semiotics movement is based, for English-speakers at least, on only a few translated articles, selected mostly from the earlier, experimental, stage of his thinking. (For Lotman's early works on the semiotics of culture available in English, see Lotman 1967, 1968, 1969; for German translations of his work including the most recent, see Eimermacher 1971, 1975; for Italian translations, see Lotman 1973; for Hungarian, Lotman 1973c; and for Polish, Semiotyka 1974. For a discussion of Lotman's work on the theory of literature, see Shukman 1976 and forthcoming.},
 author = {Ann Shukman},
 doi = {https://doi.org/10.1016/0304-3479(77)90019-9},
 issn = {0304-3479},
 journal = {Russian Literature},
 number = {1},
