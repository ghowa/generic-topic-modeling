 note = {Sergei Tret'iakov: The New Visuality, Art and Document},
 pages = {235-258},
 title = {Furrows of the Field â Lines of the Gaze: Central-Perspective Figurations of Subjectivity in USSR in Construction (1930â1940)},
 url = {https://www.sciencedirect.com/science/article/pii/S0304347919300109},
