@article{BROOKS2016113,
 abstract = {This article considers the structure of Vladislav Khodasevich's Necropolis (Nekropol', 1939) in relation to the volume's critique of Symbolist life-creation (zhiznetvorchestvo). Necropolis compiles short memoir-portraits of various turn-of-the-century Russian writers. These acerbic pieces variously implicate life-creative behavioral codes in many of these Symbolist writers' tragic fates. I contend that the sequence in which these portraits appear enhances Necropolis' critique. The opening trio of portraits (on Nina Petrovskaia, Valerii Briusov, and Andrei Belyi) compel the reader to repeatedly experience the collapse of those writers' love triangle. Khodasevich thus highlights two things: the degree to which individual Symbolist lives are inevitably âtangledâ (in Khodasevich's words) with one another; and the life-creative mandate of reliving one-time, kairotic events. I also argue that Khodasevich's portrait of Maksim Gor'kii (the final piece in Necropolis) represents a counterpoint to the preceding Symbolist lives. Gor'kii's life is just as âtangledâ and artificial; however, he recognizes the falsity of his life-creative biography, and embraces the revisionist âennobling truthâ that Khodasevich offers him. Gor'kii's portrait thus throws Necropolis' more famous Symbolist content into relief, and demonstrates Khodasevich's prerogative to write his contemporariesâ biographies. Thus, Necropolis' critique of life-creation becomes more meaningful when one reads its constituent pieces as a unified whole.},
 author = {Daniel Brooks},
 doi = {https://doi.org/10.1016/j.ruslit.2016.08.007},
 issn = {0304-3479},
 journal = {Russian Literature},
 keywords = {V.F. Khodasevich, âNecropolisâ, Symbolism, Life-Creation},
 note = {V.F. ChodaseviÄ},
 pages = {113-128},
 title = {Vladislav ChodaseviÄ's Necropolis: Writing Lives in the City of the Dead},
 url = {https://www.sciencedirect.com/science/article/pii/S0304347916300230},
 volume = {83-84},
 year = {2016}
}

