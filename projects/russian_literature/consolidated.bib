 note = {Censorship in Nineteenth-Century Russia},
 pages = {87-114},
 title = {âWondrous Lifeâ: Dobroliubovâs âOrganicâ Reception of Chernyshevskiiâs Rationalist Aesthetics},
 url = {https://www.sciencedirect.com/science/article/pii/S0304347920300235},
 volume = {113},
 year = {2020}
}

