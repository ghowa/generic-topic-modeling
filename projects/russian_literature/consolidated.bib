 author = {Vladimir Feshchenko},
 doi = {https://doi.org/10.1016/j.ruslit.2011.08.006},
 issn = {0304-3479},
 journal = {Russian Literature},
 keywords = {Khlebnikov, Language, Experiment, Life-Creation},
 number = {2},
 pages = {259-290},
 title = {Breakthrough into Languages. Velimir ChlebnikovÊ¼s âLanguage Husbandryâ and Life-Creation},
 url = {https://www.sciencedirect.com/science/article/pii/S0304347911000317},
 volume = {69},
 year = {2011}
}

